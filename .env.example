# Data folder path
DATA_FOLDER=/root/n8n/n8n-docker-caddy

# Domain configuration
DOMAIN_NAME=example.com
SUBDOMAIN=n8n
PORT=5678

# Server configuration (uncomment if needed)
#SERVER_IP=YOUR_SERVER_IP

# Timezone setting
GENERIC_TIMEZONE=UTC

# SSL configuration
SSL_EMAIL=your-email@example.com

# Database configuration
POSTGRES_USER=n8n_user
POSTGRES_PASSWORD=change_me
POSTGRES_DB=n8n

# N8N security keys (change these!)
N8N_ENCRYPTION_KEY=change_this_encryption_key
N8N_USER_MANAGEMENT_JWT_SECRET=change_this_jwt_secret
